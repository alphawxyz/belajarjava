public class TipeDataChar {
    public static void main(String[] args) {

        char a = 'A';
        char d = 'D';
        char i = 'I';

        System.out.println(a);
        System.out.println(d);
        System.out.println(i);

        //Pintln maka akan ditambahkan baris/enter jika dengan print saja maka tidak menambah baris enter
        System.out.print(a);
        System.out.print(d);
        System.out.print(i);

    }
}
